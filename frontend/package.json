{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@maykin-ui/admin-ui": "^0.0.40",
    "@storybook/test-runner": "^0.18.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.97",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "date-fns": "^3.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "react-scripts": "^5.0.1",
    "react-use": "^17.5.0",
    "tinyduration": "^3.3.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "npm test -- --coverage --coverageDirectory=coverage/jest --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --coverage",
    "prepare": "cd .. && husky frontend/.husky",
    "lint": "eslint .",
    "lint:fix": "eslint --fix src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{css,scss}": "npx prettier -w",
    "*.{js,jsx,ts,tsx,md}": "npm run lint:fix"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@maykinmedia/eslint-config": "^1.0.0",
    "@playwright/test": "^1.48.0",
    "@storybook/addon-coverage": "^1.0.4",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/addon-onboarding": "^8.0.10",
    "@storybook/blocks": "^8.0.10",
    "@storybook/preset-create-react-app": "^8.0.10",
    "@storybook/react": "^8.0.10",
    "@storybook/react-webpack5": "^8.0.10",
    "@storybook/test": "^8.0.10",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.14.0",
    "husky": "^9.0.11",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "prop-types": "^15.8.1",
    "storybook": "^8.0.10",
    "storybook-addon-mock": "^5.0.0",
    "storybook-addon-module-mock": "^1.3.4",
    "typescript-eslint": "^8.19.0",
    "webpack": "^5.91.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      ".*\\.test\\.tsx?",
      ".*\\.stories\\.tsx?"
    ]
  }
}

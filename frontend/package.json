{
  "name": "frontend",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "@maykin-ui/admin-ui": "^2.0.0-alpha.6",
    "@storybook/react-vite": "^8.6.4",
    "@storybook/test-runner": "^0.22.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "date-fns": "^3.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.23.0",
    "react-use": "^17.5.0",
    "tinyduration": "^3.3.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "OAB_API_URL=http://localhost:8000 vitest",
    "test:coverage": "OAB_API_URL=http://localhost:8000 vitest run --coverage --watch=false",
    "storybook": "OAB_API_URL=http://localhost:8000 storybook dev -p 6006",
    "build-storybook": "OAB_API_URL=http://localhost:8000 storybook build",
    "test-storybook": "test-storybook --coverage",
    "prepare": "cd .. && husky frontend/.husky",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{css,scss}": "npx prettier -w",
    "*.{js,jsx,ts,tsx,md}": "npm run lint:fix"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@maykinmedia/eslint-config": "^1.0.0",
    "@playwright/test": "^1.48.0",
    "@storybook/addon-coverage": "^1.0.4",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-istanbul": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.4",
    "globals": "^15.14.0",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.7.3",
    "prettier": "3.2.5",
    "prop-types": "^15.8.1",
    "storybook": "^8.6.4",
    "storybook-addon-mock": "^5.0.0",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.2.1",
    "vitest": "^3.0.8",
    "webpack": "^5.91.0"
  },
  "overrides": {
    "storybook-addon-mock": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
